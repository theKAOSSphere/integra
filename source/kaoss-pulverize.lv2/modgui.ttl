@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/theKAOSSphere/integra#pulverize>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-pulverize.html> ;
        modgui:stylesheet <modgui/stylesheet-pulverize.css> ;
        modgui:screenshot <modgui/screenshot-pulverize.png> ;
        modgui:thumbnail <modgui/thumbnail-pulverize.png> ;
        modgui:brand "KAOSS" ;
        modgui:label "Pulverize" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "tribal2" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Volume" ;
            lv2:name "Volume" ;
        ] ;
    ] .